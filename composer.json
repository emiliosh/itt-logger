{
    "name": "emiliosh/itt-logger",
    "description": "Opensearch logger handler",
    "keywords": ["Opensearch", "Logger"],
    "type": "library",
    "require": {
        "laravel/framework": "^8.0|^9.0|^10.0",
        "predis/predis": "^1.1",
        "monolog/monolog": "^2.0",
        "opensearch-project/opensearch-php": "^2.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ch",
            "email": "chernikov_an@it-thematic.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Itt\\Logger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Itt\\Logger\\Providers\\LoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
